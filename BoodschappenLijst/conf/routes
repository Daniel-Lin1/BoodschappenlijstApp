# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

GET     /account/loginredirect      controllers.AccountController.loginRedirect
GET     /account/register           controllers.AccountController.register
GET     /account/login              controllers.AccountController.login
POST    /account/login              controllers.AccountController.authenticate
POST    /account/submitRegister     controllers.AccountController.submitRegister

GET     /getResults                 controllers.Application.getResults

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

